CREATE(25, 100, 500; 2)
CONFIGURE([0] "Coke", 50; "Oxygen", 250)
CREATE(10, 25; 1)
CONFIGURE([1] "Oxygen", 20)
COIN_LOAD([0] 0; 25, 2)
COIN_LOAD([1] 0; 10, 2)
POP_LOAD([0] 0; "Coke", 1)
POP_LOAD([0] 1; "Oxygen", 1)
POP_LOAD([1] 0; "Oxygen", 2)
INSERT([0] 100)
PRESS([0] 1)
PRESS([0] 0)
EXTRACT([0])
CHECK_DELIVERY(50, "Coke")
UNLOAD([0])
CHECK_TEARDOWN(0; 100; "Oxygen")
EXTRACT([1])
CHECK_DELIVERY(0)
INSERT([1] 5)
INSERT([1] 25)
PRESS([1] 0)
EXTRACT([1])
CHECK_DELIVERY(5, "Oxygen")
UNLOAD([1])
CHECK_TEARDOWN(20; 25; "Oxygen")