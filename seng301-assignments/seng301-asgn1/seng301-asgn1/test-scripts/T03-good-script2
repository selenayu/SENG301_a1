CREATE(5, 10, 25, 100, 500; 2)
CONFIGURE([0] "Coke", 50; "Oxygen", 250)
COIN_LOAD([0] 0; 5, 1)
COIN_LOAD([0] 1; 10, 10)
COIN_LOAD([0] 2; 25, 2)
COIN_LOAD([0] 3; 100, 2)
POP_LOAD([0] 1; "Oxygen", 2)
INSERT([0] 500)
INSERT([0] 500)
PRESS([0] 0)
PRESS([0] 1)
EXTRACT([0])
CHECK_DELIVERY(355, "Oxygen")
INSERT([0] 500)
PRESS([0] 1)
EXTRACT([0])
INSERT([0] 500)
CHECK_DELIVERY(0, "Oxygen")
UNLOAD([0])
CHECK_TEARDOWN(0; 1500)
UNLOAD([0])
CHECK_TEARDOWN(0; 0)